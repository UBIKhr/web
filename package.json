{
  "name": "ubik-web",
  "version": "1.0.0",
  "description": "Landing page for ubik.hr",
  "repository": "git@github.com:UBIKhr/web.git",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site -vw",
    "start:webpack": "webpack-dev-server --config webpack.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "yarn run start:hugo -- -D -F",
    "preview:webpack": "yarn run start:webpack",
    "prebuild": "rimraf dist",
    "build": "yarn run build:webpack && yarn run build:hugo",
    "build:preview": "yarn run build:webpack && yarn run build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site -v --config config.toml",
    "build:hugo:preview": "yarn run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "assets-webpack-plugin": "^7.1.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-plugin-import": "^2.22.1",
    "exports-loader": "^4.0.0",
    "fancy-log": "^2.0.0",
    "file-loader": "^6.2.0",
    "hugo-extended": "^0.109.0",
    "imports-loader": "^4.0.1",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "plugin-error": "^2.0.1",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.3.2",
    "rimraf": "^4.0.4",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "whatwg-fetch": "^3.0.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
